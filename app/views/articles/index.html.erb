<div>
  <h1>All articles</h1>
</div>

<div>
  <ul class="fa-ul">
    <% @articles.each do |article| %>
      <li>
        <i class="fa-li fa fa-square"></i>
        <%= link_to article.title, article_path(article.id) %>
      </li>
    <% end %>
  </ul>
</div>

<div>
  <%= link_to "Create new article", new_article_path %>
</div>

